# Environment Configuration for data_develop_java

## Database Credentials
RAW_DB_USER=raw_user
RAW_DB_PASS=raw_pass
METRICS_DB_USER=metrics_user
METRICS_DB_PASS=metrics_pass

## JWT Security
JWT_SECRET=your-strong-secret-key-here
JWT_EXPIRATION_MS=86400000
DEMO_USERNAME=admin
DEMO_PASSWORD=admin123

## OCI Object Storage Configuration
OCI_NAMESPACE=your-oci-namespace
OCI_REGION=us-ashburn-1
OCI_BUCKET=data-platform-bucket
OCI_BASE_URL=https://objectstorage.us-ashburn-1.oraclecloud.com
OCI_EXPIRATION_SECONDS=900
OCI_CONFIG_FILE=~/.oci/config
OCI_PROFILE=DEFAULT
OCI_CONFIG_DIR=~/.oci

## Spring Profile (dev, test, prod)
SPRING_PROFILES_ACTIVE=dev

## Frontend API Base URL
VUE_APP_API_BASE_URL=http://localhost:8080
VUE_APP_STORAGE_BASE_URL=https://objectstorage.us-ashburn-1.oraclecloud.com
